{
  "name": "skatehubba-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo root for Skatehubba platform",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/myhuemungusD/skatehubbaweb010.git"
  },
  "packageManager": "pnpm@9.1.0",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "dev": "turbo dev --parallel",
    "build": "turbo build",
    "clean": "turbo clean && rm -rf node_modules **/.next **/dist **/.turbo",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "android": "turbo run android --filter @skatehubba/mobile",
    "ios": "turbo run ios --filter @skatehubba/mobile",
    "start": "echo 'Server removed. Use Firebase Functions.'"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.13.0",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "turbo": "^2.1.3",
    "typescript": "^5.6.3"
  },
  "pnpm": {
    "overrides": {
      "react": "18.3.1",
      "react-dom": "18.3.1",
      "@types/react": "18.3.12",
      "@types/react-dom": "18.3.0",
      "react-native": "0.74.5",
      "dtrace-provider": "npm:@empty/package@latest",
      "detox@* > dtrace-provider": "npm:@empty/package@latest",
      "react-native-maps@* > react-native-gesture-handler@* > dtrace-provider": "npm:@empty/package@latest"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "@types/react",
        "eslint"
      ],
      "allowedVersions": {
        "react": "18",
        "react-dom": "18"
      }
    }
  }
}
