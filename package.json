{
  "name": "skatehubba-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo root for Skatehubba platform",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/myhuemungusD/skatehubbaweb010.git"
  },
  "packageManager": "pnpm@9.1.0",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "git diff --quiet pnpm-lock.yaml || echo '\n⚠️  WARNING: pnpm-lock.yaml has uncommitted changes!\n   Run: git add pnpm-lock.yaml && git commit -m \"chore: update lockfile\"\n'",
    "dev": "turbo dev --parallel",
    "dev:server": "turbo dev --filter @skatehubba/server",
    "dev:web": "turbo dev --filter @skatehubba/web",
    "dev:landing": "turbo dev --filter @skatehubba/landing",
    "dev:mobile": "turbo dev --filter @skatehubba/mobile",
    "dev:admin": "turbo dev --filter @skatehubba/admin",
    "dev:studio": "turbo dev --filter @skatehubba/studio",
    "dev:all": "turbo dev --parallel",
    "build": "turbo build",
    "start": "NODE_ENV=production node apps/server/dist/server/index.js",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo clean && rm -rf node_modules **/node_modules **/.next **/dist **/.turbo"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "openai": "^6.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.13.0",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "turbo": "^2.1.3",
    "typescript": "^5.6.3"
  },
  "pnpm": {
    "overrides": {
      "dtrace-provider": "npm:@empty/package@latest",
      "detox@* > dtrace-provider": "npm:@empty/package@latest",
      "react-native-maps@* > react-native-gesture-handler@* > dtrace-provider": "npm:@empty/package@latest"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "@types/react",
        "eslint"
      ],
      "allowedVersions": {
        "react": "18",
        "react-dom": "18"
      }
    },
    "allowedBuildScripts": [
      "build",
      "postinstall",
      "prepare",
      "prepack"
    ]
  }
}
