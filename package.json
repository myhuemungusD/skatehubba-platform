{
  "name": "skatehubba-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@skatehubba/web",
    "dev:mobile": "turbo run dev --filter=@skatehubba/mobile",
    "dev:server": "turbo run dev --filter=@skatehubba/server",
    "dev:landing": "turbo run dev --filter=@skatehubba/landing",
    "dev:admin": "turbo run dev --filter=@skatehubba/admin",
    "dev:studio": "turbo run dev --filter=@skatehubba/studio",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules apps/*/.next apps/*/dist packages/*/dist"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/react-native": "^0.72.8",
    "firebase-functions": "^6.6.0",
    "openai": "^6.8.1",
    "react": "^18.2.0",
    "react-native": "~0.74.0"
  },
  "devDependencies": {
    "turbo": "^1.12.0",
    "typescript": "~5.3.0"
  }
}
