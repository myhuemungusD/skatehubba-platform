    var defaultTextMapGetter = {
      get: function (carrier, key) {
        if (carrier == null) {
          return undefined;
        }
        return carrier[key];
      },
      keys: function (carrier) {
        if (carrier == null) {
          return [];
        }
        return Object.keys(carrier);
      }
    };
    var defaultTextMapSetter = {
      set: function (carrier, key, value) {
        if (carrier == null) {
          return;
        }
        carrier[key] = value;
      }
    };

